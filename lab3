#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() {
    char x[81];
    printf("Vvedite simvoli: ");
    scanf("%80s", x);

    for (int i = 0; i < strlen(x); i++) {
        if (x[i] == 'a' || x[i] == 'b') {
            x[i] = toupper(x[i]);
        }
    }

    printf("vivod: %s\n", x);
    return 0;
} 
