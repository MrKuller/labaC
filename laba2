#include <stdio.h>
#include <math.h>
int main(){
    double arr[3][3];
    int q,w;
    printf("Vvedite znach: \n");
    for (int q = 0; q < 3; q++){
        for (int w = 0; w<3; w++){
            scanf("%lf",&arr[q][w]);
        }
    }
    int x = 0;
    for (int i = 0; i < 3; i ++){
        for (int j = 0; j < 3; j++){
            printf("%.1f ", arr[i][j]);
            if (i == j) {
                x += arr[i][j];
            }
        }
        printf("\n");
    }
    int y = 0;
    for (int i = 0; i < 3; i++){
        y += arr[i][2-i];
    }
    printf("Summ main dia: %d\n", x);
    printf("Summ vtor dia: %d\n", y);
    return 0;
}



''''''''''''''''''''''''''''''''''''''''''''''''''

int main(){
    int arr[2][2];
    int q,w,i,j;
    int x = 0;
    printf("Vvedite znach: \n");
    for (int q = 0; q < 2; q++){
        for (int w = 0; w<2; w++){
            scanf("%lf",&arr[q][w]);
        }
    }
    for (i = 0; i <2;i++){
        for (j=0;j<2;j++){
            int x = arr[i][j] * arr[i][j];
        }
    }
    printf("%d", x)
    return 0;
}





