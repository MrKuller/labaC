#include <stdio.h>
int main(){
    double mass[3][3];
    int i,y;
    for (i = 0; i<3; i++){
        for (y=0; y<3; y++){
            printf("mass[%d][i] = ", i);
            scanf("%d", &mass[y][i]);
        }
    }
    int x = 0;
    for (int i = 0; i < 3; i ++){
        for (int j = 0; j < 3; j++){
            printf("%.1f ", mass [i][j]);
            if (i == j) {
                x += mass[i][j];
            }
        }
        printf("\n");
    }
    printf("Summ main dia: %d\n", x);

    return 0;
}
